{
  "name": "shadcnify",
  "version": "0.0.0",
  "packageManager": "bun@1.3.4",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@repo/web",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  }
}
